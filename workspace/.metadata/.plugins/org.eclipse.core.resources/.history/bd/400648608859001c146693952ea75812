<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.puppy.admin.room.dao.AdminRoomDAO">
	<resultMap type="com.puppy.admin.room.vo.AdminRoomVO" id="adminRoomVO">
		<result property="c_no" column="c_no"></result>
		<result property="c_kind" column="c_kind"></result>
		<result property="c_type" column="c_type"></result>
		<result property="c_price" column="c_price"></result>
		<result property="c_explain" column="c_explain"></result>
		<result property="c_picture" column="c_picture"></result>
		<result property="c_status" column="c_status"></result>
		<result property="c_usestatus" column="c_usestatus"></result>
	</resultMap>
	
	<select id="roomList" parameterType="com.puppy.admin.room.vo.AdminRoomVO" resultMap="adminRoomVO">
		select c_no,c_kind,c_type,c_price,c_status,c_usestatus 
		from cageroom_tbl 
		order by c_no asc
	</select>
	
	<select id="roomDetail" parameterType="com.puppy.admin.room.vo.AdminRoomVO" resultMap="adminRoomVO">
		select c_no,c_kind,c_type,c_price,c_explain,c_picture,c_status,s_name
		from cageroom_tbl 
		order by c_no asc
	</select>
</mapper>